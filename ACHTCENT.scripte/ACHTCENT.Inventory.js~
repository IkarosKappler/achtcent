ACHTCENT.Inventory = function( id ) {

    ACHTCENT.Object.call();
    
    this.setID( id );
    
    // An array for storing the depositables (bottles, cans, ...)
    this.depositables = [];

}

ACHTCENT.Inventory.prototype = new ACHTCENT.Inventory( null );
